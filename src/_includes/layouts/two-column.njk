
   
{% extends 'layouts/base.njk' %}

{# Intro content #}
{% set introHeading = title %}
{% set introSummary %}
  {{ intro | safe }}
{% endset %}

{% block content %}
  <main id="main-content" tabindex="-1">
    <article class="[ post ] [ h-entry ]">
      {% include "partials/components/intro.njk" %}
      <div class="[ post__body ][ leading-loose pad-top-900 pad-bottom-900 text-500 ] [ sf-flow ] [ e-content ] [ inner-wrapper ][ two-column-grid ]">
         <div>
           {{ content | safe }}
         </div>
          <aside>
           {{ aside | safe }}
          </aside>
       </div>
    </article>
  </main>
{% endblock %}

{{ content | safe }}
